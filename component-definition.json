            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "cards": [
                    {
                      "description": "<p>One of Qiddiya Investment Company’s main goals is to establish fun-filled destinations for thrill-seekers, including both locals and visitors.</p>",
                      "image_alt": "Six Flags Qiddiya City offers thrilling rides and immersive attractions.",
                      "image_src": "/content/dam/aemigrate/uploaded-folder/image/card1-home.webp",
                      "title": "Entertainment"
                    },
                    {
                      "description": "<p>Qiddiya Investment Company promotes sports by developing facilities, establishing strategic partnerships, and hosting sporting events of all kinds—including esports and motorsports—thus catering to athletes, fans, and the industry at large.</p>",
                      "image_alt": "Qiddiya Speed Park Track at night, built for world-class motorsport and high-speed thrills.",
                      "image_src": "/content/dam/aemigrate/uploaded-folder/image/card2-home.webp",
                      "title": "Sports"
                    },
                    {
                      "description": "<p>This is the stage on which the Kingdom showcases its rich culture and traditions to the world and the bridge that connects us to the wonderful things the rest of the world has to offer.</p>",
                      "image_alt": "Illuminated Performing Arts Centre situated on the edge of the Tuwaiq Mountains.",
                      "image_src": "/content/dam/aemigrate/uploaded-folder/image/card3-home.webp",
                      "title": "Culture"
                    }
                  ],
                  "cta_link": "/qiddiya-city/",
                  "cta_text": "Explore Qiddiya City",
                  "heading": "<h2>Bringing entertainment, sports, and culture to the forefront</h2>",
                  "model": "card-list",
                  "name": "card-list"
                }
